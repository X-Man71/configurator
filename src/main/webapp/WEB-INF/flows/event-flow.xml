<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
      	http://www.springframework.org/schema/webflow
      	http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="event" class="de.hs.furtwangen.bam.jee.configurator.model.Event" />

	<!-- ID Bezeichnet den Zustand innerhalb des Flows -->
	<!-- View Attribut definiert die View die dem Benutzer angezeigt werden 
		soll. Bsp intro liefert dem Benutzer intro.html zurÃ¼ck -->
	<view-state id="intro" view="intro">
		<binder>
			<binding property="name" />
		</binder>
		<transition on="proceed" to="secoundFlowPage">
		</transition>
	</view-state>


	<view-state id="secoundFlowPage" view="secoundFlowPage">
	<binder>
			<binding property="name" />
		</binder>
	
		<transition on="proceed" to="reviewBooking" />
	</view-state>




</flow>
